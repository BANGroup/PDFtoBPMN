RACI_ROLES=$(head -n 1 "$RACI_FILE" | grep -o "|" | wc -l)
RACI_ROLES=$((RACI_ROLES - 2))  # –ú–∏–Ω—É—Å —Å—Ç–æ–ª–±—Ü—ã "Activity" –∏ –∫—Ä–∞–π–Ω–∏–µ
